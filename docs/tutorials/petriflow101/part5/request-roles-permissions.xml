		<title>Email</title>
		<validations>
			<validation>
				<expression>regex ^[\w-\.]+@([\w-]+\.)+[\w-]{2,4}$</expression>
				<message>Please type a valid email</message>
			</validation>
		</validations>
	</data>
	<data type="text">
